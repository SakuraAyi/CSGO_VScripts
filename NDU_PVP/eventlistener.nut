E_takeover <- function (eventdata)
{
    local user = GetPlayerByUserid(eventdata.userid);
    local bot = GetPlayerByUserid(eventdata.botid);
    local sc;
    
    for ( local i = 0; i < BOT.len(); i++ )
    {
        if ( i < 5 )
        {
            if ( BOT[i][0] == bot )
            {
                sc = bot.GetScriptScope();
                BOT[i][8] = false;
                BOT[i][0] = null;
                ::VS.SendToServer("bot_kick "+::GetNameByUserid(sc.userid));
                if ( ::ZmEyeGlowArray[i][0] != null && ::ZmEyeGlowArray[i][0].IsValid() )
                {
                    ::ZmEyeGlowArray[i][0].Destroy();
                    ::ZmEyeGlowArray[i][1].Destroy();
                }
                if ( ::BOT[i][16] != null && ::BOT[i][16].IsValid() )
                {
                    BOT[i][16].Destroy();
                }
            }
        }
        else
        {
            if ( BOT[i][0] == user )
            {
                local model;
                BOT[i][8] = true;
                if ( ::isWave[5] )
                {
                    model = zombie_model[31];//bot.GetModelName();

                    local ent;
                    for ( ent = Entities.First(); ent != null; ent = Entities.Next(ent) )
                    {
                        if ( ent.GetClassname().find("xm1014",0) != null )
                        {
                            if ( ent.GetMoveParent() == user )
                            {
                                ::VS.SetSpeed(user, 0, 0.7);
                                EntFireByHandle(user, "Skin", "1", 0.00, null, null);
                            }
                            else
                            {
                                ::VS.SetSpeed(user, 0, 0.9);
                                EntFireByHandle(user, "Skin", "0", 0.00, null, null);
                            }
                        }
                    }
                    
                }
                else
                {
                    model = zombie_model[12];
                    ::VS.SetSpeed(user, 2, zombie_sprint[0]);
                }
                user.SetModel(model);
                
                EntFireByHandle(user, "SetBodyGroup", "0", 0.00, null, null);
            }
        }
    }
};