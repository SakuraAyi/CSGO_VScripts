    local speed = null;
    local yaw = null;
    local tl,tl2,tl3,tl4;
    local human = null;
    local ent_2 = null;
    //printl("weapon > "+wpn);
    for (local i=0;i<BOT.len();i++) {
        if (BOT[i][0] == p) {
            vicid = i;
            break;
        }
    }
    local move = BOT[vicid][11];
    local atking = BOT[vicid][10];
    if (atk != null) {
        if (atk.GetTeam() == 3) {
            for (local i=0;i<Player.len();i++) {
                if (Player[i][0] == atk) {
                    atkid = i;
                    break;
                }
            }
            if (p.GetModelName() == ::zombie_model[23]) 
            {
                pos = p.GetOrigin();
                pos_2 = p.GetOrigin() + p.GetForwardVector() * 6;
                pos_3 = p.GetOrigin() + p.GetForwardVector() * 80;
                ang = p.GetAngles();
                speed = p.GetVelocity();
                yaw = (ang.y)*PI/180.0;	
                //p.SetOrigin(Vector(pos.x,pos.y,pos.z+1));
                //p.SetVelocity(Vector(cos(yaw)*400,sin(yaw)*400,200));
                tl = TraceLine(pos,Vector(pos_3.x,pos_3.y,pos_3.z+32),p);
                tl2 = TraceLine(pos,Vector(pos_3.x,pos_3.y,pos_3.z+64),p);
                tl3 = TraceLine(pos,Vector(pos_3.x,pos_3.y,pos_3.z+80),p);
                tl4 = TraceLine(pos,pos_3,p);
                if (tl==1.0 && tl2==1.0 && tl3==1.0 && tl4==1.0) {
                    while (ent_2 = Entities.FindByClassnameWithin(ent_2, "*", pos_2, 64)) {
                            if (ent_2.GetClassname() == "player") {
                            human = ent_2;
                            if (human.GetTeam() == 3) {
                                printl("There are player");
                                return;
                            }
                        }
                    }
                    if (move && !atking) {
                        p.SetOrigin(pos_2);
                        printl("speed up");
                    }
                }
                else {
                    printl("There is a wall");
                    return;
                }
            }
            else if ( p.GetModelName() == ::zombie_model[31] )
            {
                if ( atk != p && HurtSnd_Z[vicid] )
                {
                     local sndent = Entities.FindByName(null, "zombie_snd_id"+vicid);
                     if ( sndent != null ) 
                     {
                         local r = RandomInt(1,14),snd;
                         snd = ::ATC_SND[8]+r+".mp3"
                         EntFireByHandle(sndent, "Volume", "0", 0.00, null, null);
                         EntFireByHandle(sndent, "StopSound", "", 0.00, null, null);
                         EntFireByHandle(sndent, "AddOutput", "message "+snd, 0.00, null, null);
                         EntFireByHandle(sndent, "PlaySound", "", 0.00, null, null);
                         EntFireByHandle(ent, "RunScriptCode", "HurtSndSwitch_Z("+vicid+")", 3.0, null, null);
                         HurtSnd_Z[vicid] = false;
                     }
                }
            }

            if ( wpn == "inferno" )
            {
                hurt = Entities.CreateByClassname("point_hurt");
                hurt.__KeyValueFromInt("DamageType", 8);
                tg = p.GetName();
                hurt.__KeyValueFromString("DamageTarget", tg);
                hurt.SetOwner(atk);
                hurt.__KeyValueFromInt("Damage", dmg*5);
                EntFireByHandle(hurt, "AddOutput", "classname weapon_incgrenade", 0.00, null, null);
                EntFireByHandle(hurt, "Hurt", "", 0.00, atk, null);
                EntFireByHandle(hurt, "Kill", "", 0.00, null, null);
                printl(p.GetHealth().tostring());
            }

            sc = atk.GetScriptScope();
            if( wpn != "point_hurt" && ::secondary_damage[atkid] ) 
            {
                ::secondary_damage[atkid] = false;
                EntFire("Script", "RunScriptCode", "::secondary_damage["+atkid+"] = true", 0.10);

                local pass = false,setdmg = null;
                if ( sc._class == career.rook )
                {
                    for ( local i = 0; i < ::Heavy.len(); i++ )
                    {
                        if ( wpn == "xm1014" )
                        {
                            pass = true;
                            printl("rook_buff");
                            setdmg = dmg*18;
                            break;
                        }
                        else
                            continue;
                    }
                }
                else if ( sc._class == career.assault )
                {
                    for ( local i = 0; i < ( ::Rifle.len() + ::Pistol.len() ); i++ )
                    {
                        if ( i < ::Rifle.len() && wpn == Rifle[i] || i < ::Pistol.len() && wpn == ::Pistol[i] )
                        {
                            pass = true;
                            printl("rifle_pistol_buff");
                            if ( wpn == "aug" || wpn == "sg556" )
                                setdmg = (dmg*2);
                            else if ( wpn == "deagle" )
                                setdmg = (dmg*3);
                            else
                                setdmg = (dmg+25);
                            break;
                        }
                        else
                            continue;
                    }
                }
                else if ( sc._class == career.support )
                {
                    for ( local i = 0; i < ::Heavy.len(); i++ )
                    {
                        if ( wpn == ::Heavy[i] )
                        {
                            pass = true;
                            printl("support_buff");
                            if ( wpn == "nova" || wpn == "sawedoff" || wpn == "xm1014" ) // Eight bullets
                                setdmg = dmg*8;
                            else if ( wpn == "mag7" )
                                setdmg = dmg*16;
                            else
                                setdmg = (dmg*3);
                            break;
                        }
                        else
                            continue;
                    }
                }
                else if ( sc._class == career.doctor )
                {
                    for ( local i = 0; i < ::SMG.len(); i++ )
                    {
                        if ( wpn == ::SMG[i] )
                        {
                            pass = true;
                            printl("smg_buff");
                            setdmg = (dmg*2);
                            break;
                        }
                        else
                            continue;
                    }
                }
                else if ( sc._class == career.trap )
                {
                    for ( local i = 0; i < ::Sniper.len(); i++ )
                    {
                        if ( wpn == ::Sniper[i] )
                        {
                            pass = true;
                            printl("sniper_buff");
                            if ( wpn == "awp" )
                                setdmg = (dmg*4);
                            else
                                setdmg = (dmg*2);
                            break;
                        }
                        else
                            continue;
                    }
                    if ( wpn == "breachcharge" )
                    {
                        pass = true
                        setdmg = (dmg*4);
                    }
                }

                if ( !pass )
                    return;

                hurt = Entities.CreateByClassname("point_hurt");
                hurt.__KeyValueFromInt("DamageType", 2);
                tg = p.GetName();
                hurt.__KeyValueFromString("DamageTarget", tg);
                hurt.SetOwner(atk);

                EntFireByHandle(hurt, "AddOutput", "classname weapon_"+wpn, 0.00, null, null);
                hurt.__KeyValueFromInt("Damage", setdmg);
                EntFireByHandle(hurt, "Hurt", "", 0.00, atk, null);
                EntFireByHandle(hurt, "Kill", "", 0.00, null, null);

                printl("oneshot_damage > "+dmg);
                printl("oneshotdamage_total > "+(dmg+setdmg));
                printl("shotgundamage_total > "+((dmg*8)+setdmg));
                return;
            }
        }
    }
}

