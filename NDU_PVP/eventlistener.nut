                shak.__KeyValueFromFloat("duration", 2);
                shak.__KeyValueFromInt("frequency", 40);
                shak.__KeyValueFromInt("radius", 512);
                shak.SetOrigin(pos);
                EntFireByHandle(shak, "StartShake", "", 0.00, null, null);
                //EntFireByHandle(shak, "StopShake", "", 2.00, null, null);
                EntFireByHandle(shak, "Kill", "", 0.01, null, null);
                local ent = null;
