    local model,snd,file,r,exp,shak;
    for ( local i = 0 ; i < Player.len() ; i ++ ) {
        if (Player[i][0] == p) {
            id = i;
            break;
        }
    }
    while ((model = Entities.FindByClassname(model, "predicted_viewmodel")) != null) {
        if (model.GetModelName().find("deagle",0) != null) {
            if (model.GetMoveParent() == p) {
                //DispatchParticleEffect("explosion_molotov_air", pos, Vector(0,0,0));
                DispatchParticleEffect("explosion_molotov_air_splash01a", pos, Vector(0,0,0));
                DispatchParticleEffect("explosion_hegrenade_brief", pos, Vector(0,0,0));
                snd = Entities.FindByName(null, "map_world_player_sound_"+id);
